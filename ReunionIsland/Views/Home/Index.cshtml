    @*CONTACT*@
    <a name="ScrollToContact"></a>
    <div class="banner">
        <div class="container">
            <div class="col-lg-12">
                <h2>Get in touch today</h2>
            </div>
            <div class="col-lg-12">
                <ul class="list-inline banner-social-buttons">
                    <li>
                        <a href="https://twitter.com/SBootstrap" class="btn btn-default btn-lg"><i class="fa fa-twitter fa-fw"></i> <span class="network-name">Twitter</span></a>
                    </li>
                    <li>
                        <a href="https://github.com/IronSummitMedia/startbootstrap" class="btn btn-default btn-lg"><i class="fa fa-github fa-fw"></i> <span class="network-name">Github</span></a>
                    </li>
                    <li>
                        <a href="#" class="btn btn-default btn-lg"><i class="fa fa-linkedin fa-fw"></i> <span class="network-name">Linkedin</span></a>
                    </li>
                </ul>
            </div>
        </div>
        @Html.Partial("~/Views/Home/_Contact.cshtml")
    </div>

    @*MODAL POPUP*@
    <div id="successModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="successModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="text-center">
                    <div class="modal-header">
                        <img id="imageresource" src="../Content/img/contact-us.png">
                        <h3>Your WebDev Agency</h3>
                    </div>
                    <div>

                    </div>
                </div>
                <div class="modal-body">
                    <h4 id="showSuccessMessage" class="modal-title text-center">@Html.Raw(ViewBag.ShowSuccessMessage)</h4>
                </div>
                <div class="modal-footer">
                    <div class="text-center">
                        <div class="btn-group btn-group-sm">
                            <button type="button" class="btn btn-sm btn-primary btn-raised" data-dismiss="modal">Continue on the site...</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*SPINNER LOADING*@
    <div id="loading">
